                            @Html.DropDownList("ddlIdEmpresa", (IEnumerable<SelectListItem>)ViewBag.Empresa, null, new { @class = "form-control", @Title = "Selecione uma empresa, para apresentar Aplicações", @onchange = "fnCarregaAplicacao(this, 'ddlIdAplicacao');" })
                        </div>

                        <div class="form-group col-md-6">
                            <label class="control-label"><strong>*</strong>Aplicacao</label>
                            @Html.DropDownList("ddlIdAplicacao", (IEnumerable<SelectListItem>)ViewBag.Aplicacao, "*** SELECIONE UMA EMPRESA ***", new { @class = "form-control", @Title = "Selecione uma aplicação, para apresentar Usuário e Módulos" }) <!--|, onchange = "fnCarregaUsuario(this, 'ddlUsuario'); fnCarregaModulo(this, 'ddlModulo');"|-->
                        </div>
                        <div class="clearfix"></div>
                    </div>

                    <div class="form-group col-md-4">
                        <label class="control-label">Usuário</label>
                        @Html.DropDownList("ddlUsuario", (IEnumerable<SelectListItem>)ViewBag.Aplicacao, "*** SELECIONE UM USUÁRIO ***", new { @class = "form-control", @Title = "Selecione para obter informações geradas por um determinado Usuário", @onchange = "fnCarregaDataTable_TipoLog(this, 'tbTipoLog');" })
                    </div>

                    <div class="form-group col-md-4">
                        <label class="control-label">Módulo</label>
                        @Html.DropDownList("ddlModulo", (IEnumerable<SelectListItem>)ViewBag.Aplicacao, "*** SELECIONE ***", new { @class = "form-control", @Title = "Selecione para obter informações geradas em determinado Módulo", @onchange = "fnCarregaDataTable_TipoLog(this, 'tbTipoLog');" })
                    </div>

                    <div class="form-group col-md-2">
                        <label class="control-label">Incluir Inativos</label>
                        @Html.DropDownList("ddlInativo", new SelectList(PM.LogAndAlert.Library.Helper.IsAtivo(true), "Key", "Value"), "** SELECIONE **", new { @class = "form-control", @Title = "Deseja incluir na pesquisa registros sinalizados como inativos?" })
